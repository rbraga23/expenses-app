{
  "name": "expenses-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "tsx src/server.ts",
    "build": "tsc && tsc-alias",
    "typeorm": "tsx -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:run": "npm run typeorm migration:run -- -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "seed:run": "tsx src/database/seeds/main.seed.ts",
    "test:migration:run": "NODE_ENV=test npm run typeorm migration:run -- -d src/database/data-source.ts",
    "test:migration:revert": "NODE_ENV=test npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "test:seed:run": "NODE_ENV=test tsx src/database/seeds/main.seed.ts",
    "test": "DOTENV_CONFIG_PATH=.env.test jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "mysql2": "^3.9.2",
    "reflect-metadata": "^0.2.1",
    "resend": "^3.2.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
